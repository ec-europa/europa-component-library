{
  "name": "@ec-europa/europa-component-library",
  "author": "ec-europa",
  "license": "EUPL-1.1",
  "version": "0.0.1",
  "description": "Europa Component Library",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build:search": "node ./scripts/search.js",
    "build": "npm-run-all --parallel build:*",
    "dist:fractal": "fractal build",
    "dist:framework": "webpack -p",
    "dist": "npm run clean && npm run build && cross-env NODE_ENV=production npm-run-all --parallel dist:*",
    "clean": "rimraf tmp build dist static/framework",
    "commitmsg": "validate-commit-msg",
    "gh-pages": "gh-pages -d dist",
    "lint:js": "eslint .",
    "lint:scss": "stylelint './framework/**/*.scss'",
    "lint": "npm-run-all --parallel lint:* -c",
    "precommit": "lint-staged",
    "start": "npm run clean && npm run build && cross-env server=true webpack-dashboard -- fractal start",
    "test:coding-conventions": "npm run lint",
    "test:functional": "wdio test/wdio.conf.js",
    "test": "npm-run-all --parallel test:* -c"
  },
  "devDependencies": {
    "@frctl/fractal": "1.1.3",
    "@frctl/mandelbrot": "1.1.0",
    "@frctl/twig": "1.0.0-alpha.0",
    "autoprefixer": "6.7.6",
    "axe-core": "2.1.7",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "chokidar-cli": "1.2.0",
    "cross-env": "3.2.3",
    "css-loader": "0.26.2",
    "cssnano": "3.10.0",
    "dotenv": "4.0.0",
    "eslint": "3.17.1",
    "eslint-config-airbnb-base": "11.1.1",
    "eslint-plugin-import": "2.2.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "fs-extra": "2.0.0",
    "gh-pages": "0.12.0",
    "html-inspector": "0.8.2",
    "husky": "0.13.2",
    "lerna": "2.0.0-beta.38",
    "lint-staged": "3.3.1",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.2",
    "stylelint": "7.9.0",
    "stylelint-config-sass-guidelines": "2.0.0",
    "stylelint-config-standard": "16.0.0",
    "stylelint-selector-bem-pattern": "1.0.0",
    "url-loader": "0.5.8",
    "validate-commit-msg": "2.11.1",
    "wdio-mocha-framework": "0.5.9",
    "wdio-sauce-service": "0.3.1",
    "wdio-screenshot": "0.5.2",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-static-server-service": "1.0.1",
    "wdio-visual-regression-service": "0.7.0",
    "webdriverio": "4.6.2",
    "webpack": "2.2.1",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1",
    "webpack-merge": "4.0.0",
    "webpack-module-hot-accept": "1.0.4"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "lunr": "1.0.0"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
